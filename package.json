{
  "name": "dev-connector",
  "version": "1.0.0",
  "description": "Full stack social network application using Node.js, Express, React, Redux and MongoDB along with ES6+.",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shinderahul/dev-connector.git"
  },
  "keywords": [
    "NodeJs",
    "Express",
    "ReactJs",
    "Redux",
    "Node API",
    "MongoDB",
    "JWT Authentication"
  ],
  "author": "Rahul Shinde",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shinderahul/dev-connector/issues"
  },
  "homepage": "https://github.com/shinderahul/dev-connector#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.20",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
